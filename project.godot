; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Science Hell"
config/description="Do you dare to gradute college?"
run/main_scene="res://science_hell/gui/Title Screen/menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://science_hell/players/hamilton/hammy.png"
config/icon="res://science_hell/players/hamilton/hammy head.png"

[autoload]

Health="*res://science_hell/gui/hearts/hearts_container.gd"
Types="*res://chey/globals/Types.gd"
ScaleManager="*res://chey/ScaleManager.gd"
SceneManager="*res://science_hell/scripts/scene_manager.gd"
Characters="*res://science_hell/gui/char_select/character_select_global.gd"
MusicManager="*res://science_hell/Music/music_manager_global.gd"
Rey="*res://Joeys level/level/reynolds_global.gd"
UI="*res://science_hell/scripts/ui_manager.gd"
Score="*res://level1(jason)/Scenes/Score.tscn"
PlayerStats="*res://science_hell/singleton/player_stats.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"HamNPC": "res://science_hell/players/HamNPC.dch",
"SongNPC": "res://addons/professors/SongNPC.dch"
}
directories/dtl_directory={
"Hamgiving": "res://science_hell/players/hamilton/Hamgiving.dtl",
"Hamwon": "res://Hamwon.dtl",
"Songgiving": "res://addons/professors/Songgiving.dtl"
}
layout/style_list=["res://addons/Style2.tres"]
layout/default_style="res://addons/Style2.tres"
glossary/default_case_sensitive=true
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}

[display]

window/stretch/mode="canvas_items"
window/stretch/scale=2.4

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/knife-tool/plugin.cfg")

[gui]

theme/custom_font="res://science_hell/assets/retro_computer_personal_use.ttf"

